public class Main {
    public static void main(String[] args) {
        int a = 5;
        int b = 7;
        System.out.println(sum(a, b));
    }

    public static int sum(int a, int b) {
        if (b == 0) {
            return a;
        } else {
            return sum(a + 1, b - 1);
        }
    }
}
